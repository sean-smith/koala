{
	"manifest_version": 2,
	"name": "Chrome Device Tab Manager",
	"version": "0.1",
	"permissions": [
		"sessions", 
		"tabs",
		"chrome://favicon/"
	],
	"browser_action": {
		"default_title": "Tabs open on your devices.",
		"default_icon": "images/iphone-128x128.png",
		"default_popup": "popup.html"
	},
	"icons": { 
		"48": "images/iphone-48x48.png",
		"128": "images/iphone-128x128.png"
	}
}